---
import SectionMain from "./SectionMain.astro"
import CardMovie from "./CardMovie.astro"
import {series} from "../../movies.json"
---

<SectionMain idSection="seccion_series" title="Series">
    {
        [...series].map(serie=>{
            return <CardMovie name={serie.title} image={serie.image} rating={serie.rating}/>
        })
    }
</SectionMain>